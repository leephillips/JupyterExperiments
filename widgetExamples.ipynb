    "from ipywidgets import *\n",
    "import pylab\n",
    "import numpy as np\n",
    "%matplotlib inline"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 48,
   "metadata": {},
   "outputs": [],
   "source": [
    "nx = 200\n",
    "x = np.arange(0, nx, 1)*(2*np.pi)/nx\n",
    "def nsin(n = 2.):\n",
    "    pylab.plot(np.sin(n*x))\n",
    "    "
   ]
